{
  "name": "storytime-waitlist-fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "pm2:start:prod": "npx pm2 start ecosystem.config.js --only storytime-waitlist-production",
    "pm2:start:dev": "npx pm2 start ecosystem.config.js --only storytime-waitlist-development",
    "pm2:restart:prod": "npx pm2 restart storytime-waitlist-production --update-env || npm run pm2:start:prod",
    "pm2:restart:dev": "npx pm2 restart storytime-waitlist-development --update-env || npm run pm2:start:dev",
    "pm2:stop:prod": "npx pm2 stop storytime-waitlist-production",
    "pm2:stop:dev": "npx pm2 stop storytime-waitlist-development",
    "pm2:save": "npx pm2 save"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.33.1",
    "@portabletext/react": "^6.0.0",
    "@portabletext/types": "^4.0.0",
    "@sanity/client": "^7.13.2",
    "@sanity/image-url": "^2.0.2",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-router": "^1.139.0",
    "framer-motion": "^12.23.24",
    "gsap": "^3.14.2",
    "lenis": "^1.3.16",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sonner": "^2.0.7",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@iconify/react": "^6.0.2",
    "@tanstack/router-plugin": "^1.136.18",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2"
  }
}
